{
  "name": "@mvc-works/lilac-parser",
  "version": "0.1.0",
  "description": "Toy parser in cljs",
  "main": "index.js",
  "scripts": {
    "deploy": "clj -A:release",
    "m2": "clj -A:release install",
    "watch": "shadow-cljs watch client",
    "serve": "http-server dist -s",
    "build-cdn": "shadow-cljs clj-run build.main/build-cdn",
    "build": "shadow-cljs clj-run build.main/build",
    "up": "yarn build-cdn && tiye-up",
    "page": "shadow-cljs clj-run build.main/page",
    "ln": "cd target && rm -f entry && ln -s ../entry"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/lilac-parser.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "@mvc-works/codearea": "^0.0.1",
    "http-server": "^0.12.1",
    "shadow-cljs": "^2.8.92",
    "source-map-support": "^0.5.16",
    "ws": "^7.2.3"
  },
  "dependencies": {
    "feather-icons": "^4.26.0",
    "shortid": "^2.2.15"
  }
}
